def calculate(op1 , op2 , f):
	if   f == "add":
	return op1 + op2

	elif f == "subtract":
		return op1 - op2

	elif f == "multiply":
		return op1 * op2

	elif f == "Devide":
		return op1/op2

result =calculate(2,3,"multiply")

print(result)

